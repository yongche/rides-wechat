.list-detail {
    margin-top: 24rpx;
}

.sure_pay {
    position: fixed;
    bottom: 24rpx;
    left: 24rpx;
    width: 702rpx;
    height: 88rpx;
    background: #AB956D;
    border-radius: 8rpx;
